{
  "name": "triniti/notify",
  "description": "Php library that provides implementations for triniti:notify schemas.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.4",
    "defuse/php-encryption": "^2.2",
    "gdbots/ncr": "dev-master",
    "guzzlehttp/guzzle": "^6.2.1",
    "triniti/apple-news": "dev-sy5",
    "triniti/sys": "dev-sy5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "symfony/dependency-injection": "^5.2",
    "triniti/acme-schemas": "dev-sy5",
    "triniti/dam": "dev-sy5",
    "triniti/news": "dev-sy5",
    "triniti/schemas": "dev-sy5",
    "twig/twig": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Triniti\\Notify\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Triniti\\Tests\\Notify\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
