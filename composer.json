{
  "name": "triniti/notify",
  "description": "Php library that provides implementations for triniti:notify schemas.",
  "type": "library",
  "license": "Apache-2.0",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/triniti/apple-news-php"
    }
  ],
  "require": {
    "php": ">=7.1",
    "defuse/php-encryption": "^2.2",
    "gdbots/ncr": "^2.0",
    "guzzlehttp/guzzle": "^6.2.1",
    "triniti/apple-news": "dev-sy5",
    "triniti/sys": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "symfony/dependency-injection": "^4.1",
    "triniti/acme-schemas": "^1.0",
    "twig/twig": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Triniti\\Notify\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Triniti\\Tests\\Notify\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
