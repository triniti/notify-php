{
  "name": "triniti/notify",
  "description": "Php library that provides implementations for triniti:notify schemas.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "gdbots/crypto-bundle": "~0.2",
    "gdbots/ncr": "~0.3",
    "guzzlehttp/guzzle": "^6.2.1",
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "symfony/dependency-injection": "^4.1",
    "triniti/acme-schemas": "~0.10",
    "triniti/schemas": "~0.15",
    "twig/twig": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "Triniti\\Notify\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Triniti\\Tests\\Notify\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
